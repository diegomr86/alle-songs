#tracks

:javascript
  $(function() {
    // Blog is the app name
    window.router = new Music.Routers.TracksRouter({tracks: #{@tracks.to_json.html_safe}});
    Backbone.history.start();
  });

%h1 Listing tracks

%table
  %tr
    %th Track
    %th Artist
    %th Session
    %th
    %th
    %th

  - @tracks.each do |track|
    %tr
      %td= track.name
      %td= track.artist
      %td= track.session_id
      %td= link_to 'Show', track
      %td= link_to 'Edit', edit_track_path(track)
      %td= link_to 'Destroy', track, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Track', new_track_path
